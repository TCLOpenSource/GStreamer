From: Matthew Waters <matthew@centricular.com>
Date: Wed, 14 Feb 2018 00:02:34 +1100
Subject: [PATCH 1/2] Use correct autotools versions

---
 Makefile.devel            | 8 ++++----
 libcharset/Makefile.devel | 6 +++---
 preload/Makefile.devel    | 4 ++--
 3 files changed, 9 insertions(+), 9 deletions(-)

diff --git a/Makefile.devel b/Makefile.devel
index 2ed8e63..6b1a132 100644
--- a/Makefile.devel
+++ b/Makefile.devel
@@ -4,10 +4,10 @@
 
 SHELL = /bin/sh
 MAKE = make
-AUTOCONF = autoconf-2.69
-AUTOHEADER = autoheader-2.69
-AUTOMAKE = automake-1.15
-ACLOCAL = aclocal-1.15
+AUTOCONF = autoconf
+AUTOHEADER = autoheader
+AUTOMAKE = automake
+ACLOCAL = aclocal
 GPERF = gperf
 CC = gcc -Wall
 CFLAGS = -O
diff --git a/libcharset/Makefile.devel b/libcharset/Makefile.devel
index 25e61e4..6ef6b44 100644
--- a/libcharset/Makefile.devel
+++ b/libcharset/Makefile.devel
@@ -3,9 +3,9 @@
 
 SHELL = /bin/sh
 MAKE = make
-AUTOCONF = autoconf-2.69
-AUTOHEADER = autoheader-2.69
-ACLOCAL = aclocal-1.15
+AUTOCONF = autoconf
+AUTOHEADER = autoheader
+ACLOCAL = aclocal
 CP = cp
 RM = rm -f
 
diff --git a/preload/Makefile.devel b/preload/Makefile.devel
index 30b8fda..25a5261 100644
--- a/preload/Makefile.devel
+++ b/preload/Makefile.devel
@@ -3,8 +3,8 @@
 
 SHELL = /bin/sh
 MAKE = make
-AUTOCONF = autoconf-2.69
-ACLOCAL = aclocal-1.15
+AUTOCONF = autoconf
+ACLOCAL = aclocal
 CP = cp
 RM = rm -f
 
